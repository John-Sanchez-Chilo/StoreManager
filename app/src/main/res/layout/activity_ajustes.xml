<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ajustes.Ajustes">

    <LinearLayout
        android:layout_width="413dp"
        android:layout_height="400dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.085">

        <TextView
            android:id="@+id/ajustes_datospersonales"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/purple_200"
            android:gravity="center"
            android:text="@string/ajustes_datos_personales"
            tools:text="@string/ajustes_datos_personales" />

        <Button
            android:id="@+id/button5"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@color/white"
            android:onClick="moveToCambiarDatos"
            android:text="@string/ajustes_cambiarDatosPersonales2" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/purple_200"
            android:gravity="center"
            android:text="@string/ajustes_productos" />

        <Button
            android:id="@+id/button6"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@color/white"
            android:onClick="moveToNuevoProducto"
            android:text="@string/ajustes_nuevoProducto" />

        <Button
            android:id="@+id/button14"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@color/white"
            android:onClick="moveToEliminarProducto"
            android:text="@string/ajustes_eliminarProducto" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>